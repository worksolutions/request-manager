{
  "name": "request-manager",
  "version": "0.0.1",
  "type": "module",
  "author": "Gevorg Danelyan",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "publish:patch": "npm run build && npm version patch && npm publish --access public",
    "publish:minor": "npm run build && npm version minor && npm publish --access public",
    "publish:major": "npm run build && npm version major && npm publish --access public",
    "test": "jest",
    "build": "rm -rf dist && cross-env NODE_ENV=production && npm run build:babel && npm run build:ts",
    "build:babel": "babel src -d dist --extensions '.ts,.tsx,.js,.jsx' --source-maps",
    "build:ts": "node_modules/typescript/bin/tsc"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@types/qs": "^6.9.11",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "jsonous": "^7.7.0",
    "qs": "^6.11.2"
  }
}
